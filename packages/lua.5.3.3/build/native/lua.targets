    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Label="v100 and x64 and Release" Condition="'$(PlatformToolset.ToLower())' == 'v100' And '$(Platform.ToLower())' == 'x64' And ( $(Configuration.ToLower().IndexOf('debug')) == -1 )">
